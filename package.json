{
  "name": "typed-electron-ipc",
  "version": "0.0.1-alpha.0",
  "description": "A solution for type-safe IPC communication in electron",
  "scripts": {
    "build": "tsc",
    "deploy": "tsc && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyleaupton/type-safe-ipc.git"
  },
  "keywords": [
    "electron",
    "IPC",
    "typescript",
    "type-safe",
    "typed"
  ],
  "author": "Kyle Upton <kyleaupton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyleaupton/type-safe-ipc/issues"
  },
  "homepage": "https://github.com/kyleaupton/type-safe-ipc#readme",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "electron": ">= 7.0.0",
    "typescript": ">= 5.4.0"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    "./main": {
      "import": "./dist/main.js",
      "require": "./dist/main.js"
    },
    "./renderer": {
      "import": "./dist/renderer.js",
      "require": "./dist/renderer.js"
    },
    "./shared": {
      "import": "./dist/shared.js",
      "require": "./dist/shared.js"
    }
  }
}