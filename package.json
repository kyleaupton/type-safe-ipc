{
  "name": "type-safe-ipc",
  "version": "0.0.1",
  "description": "A solution for type-safe IPC communication in electron",
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json && node scripts/distPackageJson.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyleaupton/type-safe-ipc.git"
  },
  "keywords": [
    "electron",
    "IPC",
    "typescript"
  ],
  "author": "Kyle Upton <kyleaupton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kyleaupton/type-safe-ipc/issues"
  },
  "homepage": "https://github.com/kyleaupton/type-safe-ipc#readme",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "electron": ">= 7.0.0",
    "typescript": ">= 5.4.0"
  },
  "exports": {
    "./main": {
      "import": "./dist/cjs/main.js",
      "require": "./dist/cjs/main.js",
      "types": "./dist/main.d.ts"
    },
    "./renderer": {
      "import": "./dist/cjs/renderer.js",
      "require": "./dist/cjs/renderer.js",
      "types": "./dist/renderer.d.ts"
    },
    "./shared": {
      "import": "./dist/cjs/shared.js",
      "require": "./dist/cjs/shared.js",
      "types": "./dist/shared.d.ts"
    }
  }
}